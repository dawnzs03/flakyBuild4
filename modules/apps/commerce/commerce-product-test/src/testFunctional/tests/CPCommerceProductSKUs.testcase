	@description = "This is a test for COMMERCE-12290. The admin can see the modal header updating correctly when adding multiple SKU/UOM combination to a price list"
	@priority = 5
	test CanModalHeaderUpdateCorrectlyWhenAddingMultipleSkuUOMToAPriceList {
		property custom.properties = "feature.flag.COMMERCE-11287=true";
		property portal.acceptance = "true";

		task ("Given a Minium site is created") {
			CommerceAccelerators.initializeNewSiteViaAccelerator(siteName = "Minium");
		}

		task ("And the admin adds 2 UOMs to a SKU") {
			for (var index : list "1,2") {
				CommerceJSONSKUsAPI._addCommerceUOM(
					basePrice = ${index},
					productSKU = "MIN93015",
					uomKey = "uom${index}",
					uomName = "uom${index}");
			}
		}

		task ("When the admin navigates to Commerce → Products → Product (e.g. ABS Sensor) → SKUs → SKU → Price Lists tab") {
			CommerceProducts.openProductsAdmin();

			CommerceProducts.searchAndGoToProductEntry(entryProductName = "ABS Sensor");

			CommerceEntry.gotoMenuTab(menuTab = "SKUs");

			CommerceNavigator.gotoEntry(entryName = "MIN93015");

			SelectFrame.selectFrameNoLoading(locator1 = "CommerceEntry#IFRAME_SIDE_PANEL");

			CommerceEntry.gotoMenuTab(menuTab = "Price Lists");
		}

		task ("Then 4 entries are present, 2 for each UOM") {
			for (var position : list "1,2,3,4") {
				AssertElementPresent(
					index = ${position},
					key_entryName = "Minium",
					locator1 = "CommerceEntry#TABLE_LIST_TITLE_AND_POSITION");
			}

			AssertElementNotPresent(
				key_entryName = "Master",
				locator1 = "CommerceEntry#TABLE_LIST_TITLE");
		}

		task ("When the admin clicks the plus button") {
			Click(locator1 = "Button#PLUS");
		}

		task ("Then a dropdown opens showing 2 UOM options") {
			for (var index : list "1,2") {
				AssertElementPresent(
					key_fieldName = "Add uom${index} to Price List",
					locator1 = "FormViewBuilder#OBJECT_FIELD_BUTTON_DROPDOWN");
			}
		}

		task ("When the admin clicks on the first option") {
			Click(
				key_fieldName = "Add uom1 to Price List",
				locator1 = "FormViewBuilder#OBJECT_FIELD_BUTTON_DROPDOWN");
		}

		task ("Then the modal header shows the correct text which includes the key of the first UOM") {
			SelectFrame.selectFrameTop();

			AssertElementPresent(
				key_frameTitle = "Add MIN93015 uom1 to Price List",
				locator1 = "CommerceEntry#ANY_IFRAME");
		}

		task ("When the admin clicks on the second option") {
			Click(locator1 = "CommerceEntry#MODAL_CLOSE");

			SelectFrame.selectFrameNoLoading(locator1 = "CommerceEntry#IFRAME_SIDE_PANEL");

			Click(locator1 = "Button#PLUS");

			Click(
				key_fieldName = "Add uom2 to Price List",
				locator1 = "FormViewBuilder#OBJECT_FIELD_BUTTON_DROPDOWN");
		}

		task ("Then the modal header shows the correct text which includes the key of the second UOM") {
			SelectFrame.selectFrameTop();

			AssertElementPresent(
				key_frameTitle = "Add MIN93015 uom2 to Price List",
				locator1 = "CommerceEntry#ANY_IFRAME");
		}
	}

	@description = "This is a test for COMMERCE-12289. The admin can see the modal header updating correctly when adding one SKU/UOM combination to a price list"
	@priority = 5
	test CanModalHeaderUpdateCorrectlyWhenAddingOneSkuUOMToAPriceList {
		property custom.properties = "feature.flag.COMMERCE-11287=true";
		property portal.acceptance = "true";

		task ("Given a Minium site is created") {
			CommerceAccelerators.initializeNewSiteViaAccelerator(siteName = "Minium");
		}

		task ("And the admin navigates to Commerce → Products → Product (e.g. ABS Sensor) → SKUs → SKU → Price Lists tab") {
			CommerceProducts.openProductsAdmin();

			CommerceProducts.searchAndGoToProductEntry(entryProductName = "ABS Sensor");

			CommerceEntry.gotoMenuTab(menuTab = "SKUs");

			CommerceNavigator.gotoEntry(entryName = "MIN93015");

			SelectFrame.selectFrameNoLoading(locator1 = "CommerceEntry#IFRAME_SIDE_PANEL");

			CommerceEntry.gotoMenuTab(menuTab = "Price Lists");
		}

		task ("When the admin opens the modal by clicking the plus button") {
			Click(locator1 = "Button#PLUS");
		}

		task ("Then the modal header shows the correct text without any UOM key") {
			SelectFrame.selectFrameTop();

			AssertElementPresent(
				key_frameTitle = "Add MIN93015 to Price List",
				locator1 = "CommerceEntry#ANY_IFRAME");
		}

		task ("When the admin adds a UOM to that SKU") {
			CommerceJSONSKUsAPI._addCommerceUOM(
				basePrice = 1,
				productSKU = "MIN93015",
				uomKey = "uom1",
				uomName = "uom1");
		}

		task ("And opens the modal by clicking the plus button") {
			Click(locator1 = "CommerceEntry#MODAL_CLOSE");

			SelectFrame.selectFrameNoLoading(locator1 = "CommerceEntry#IFRAME_SIDE_PANEL");

			CommerceEntry.gotoMenuTab(menuTab = "Details");

			CommerceEntry.gotoMenuTab(menuTab = "Price Lists");

			Click(locator1 = "Button#PLUS");

			SelectFrame.selectFrameTop();
		}

		task ("Then the modal header shows the correct text which includes the UOM key") {
			AssertElementPresent(
				key_frameTitle = "Add MIN93015 uom1 to Price List",
				locator1 = "CommerceEntry#ANY_IFRAME");
		}

		task ("When the admin adds Master Base Promotion from the modal") {
			SelectFrame(
				key_frameTitle = "Add MIN93015 uom1 to Price List",
				locator1 = "CommerceEntry#ANY_IFRAME");

			Click.javaScriptClick(
				key_imageName = "Master Base Promotion",
				locator1 = "Checkbox#ADAPTIVE_MEDIA_IMAGE");

			SelectFrame.selectFrameTop();

			Click(
				key_text = "Add",
				locator1 = "Button#ANY");
		}

		task ("Then the new entry is added successfully") {
			SelectFrame.selectFrameNoLoading(locator1 = "CommerceEntry#IFRAME_SIDE_PANEL");

			AssertElementPresent(
				key_entryName = "Master Base Promotion",
				locator1 = "CommerceEntry#TABLE_LIST_TITLE");
		}

		task ("When the admin opens the modal by clicking the plus button") {
			Click(locator1 = "Button#PLUS");

			SelectFrame.selectFrameTop();

			SelectFrame(
				key_frameTitle = "Add MIN93015 uom1 to Price List",
				locator1 = "CommerceEntry#ANY_IFRAME");
		}

		task ("Then ‘Master Base Promotion’ is checked") {
			AssertElementPresent(
				checkboxName = "Master Base Promotion",
				locator1 = "CommerceEntry#ANY_TABLE_CHECKBOX_CHECKED");
		}

		task ("When the admin deletes the entry Master Base Promotion") {
			SelectFrame.selectFrameTop();

			Click(locator1 = "CommerceEntry#MODAL_CLOSE");

			SelectFrame.selectFrameNoLoading(locator1 = "CommerceEntry#IFRAME_SIDE_PANEL");

			Click(
				key_value = "Master Base Promotion",
				locator1 = "CommerceEntry#BODY_VERTICAL_ELLIPSIS");

			CommerceEntry.clickNoError(menuItem = "Delete");
		}

		task ("And opens the modal by clicking the plus button") {
			Click(locator1 = "Button#PLUS");

			SelectFrame.selectFrameTop();

			SelectFrame(
				key_frameTitle = "Add MIN93015 uom1 to Price List",
				locator1 = "CommerceEntry#ANY_IFRAME");
		}

		task ("Then ‘Master Base Promotion’ is unchecked") {
			AssertElementPresent(
				checkboxName = "Master Base Promotion",
				locator1 = "CommerceEntry#ANY_TABLE_CHECKBOX_UNCHECKED");
		}
	}

	@description = "This is a test for COMMERCE-9891 UI to show External Reference code of SKU"
	@priority = 5
	test CanNotUseTheSameSKUERCForDifferentSKU {
		property portal.acceptance = "true";

		task ("Given a Minium site is created") {
			CommerceAccelerators.initializeNewSiteViaAccelerator(siteName = "Minium");
		}

		task ("And admin edits and publishes SKU ERC of a product") {
			CommerceNavigator.gotoPortlet(
				category = "Product Management",
				portlet = "Products");

			CommerceNavigator.searchEntry(entryName = "ABS Sensor");

			CommerceNavigator.gotoTableListEntry(entryName = "ABS Sensor");

			CommerceEntry.gotoMenuTab(menuTab = "SKUs");

			Click(
				key_value = "MIN93015",
				locator1 = "CommerceEntry#BODY_VERTICAL_ELLIPSIS");

			MenuItem.click(menuItem = "Edit");

			CommerceSKUs.editSkuERC(
				closeFrame = "true",
				publish = "true",
				skuERC = "Test ERC");
		}

		task ("When enters the same ERC value to another product and clicks Publish") {
			Click(locator1 = "CommerceNavigation#BACK");

			CommerceNavigator.searchEntry(entryName = "U-Joint");

			CommerceNavigator.gotoTableListEntry(entryName = "U-Joint");

			CommerceEntry.gotoMenuTab(menuTab = "SKUs");

			Click(
				key_value = "MIN55861",
				locator1 = "CommerceEntry#BODY_VERTICAL_ELLIPSIS");

			MenuItem.click(menuItem = "Edit");

			CommerceSKUs.editSkuERC(
				publish = "true",
				skuERC = "Test ERC");
		}

		task ("Then verifies that the same ERC cannot be given twice and an error message appears") {
			AssertElementNotPresent(locator1 = "Message#SUCCESS");

			AssertTextEquals.assertText(
				locator1 = "Message#ERROR",
				value1 = "Close Error:There is already one SKU with the external reference code.");
		}
	}

	@description = "This is a test for COMMERCE-10604. Can patch a SKU's ERC via API"
	@priority = 3
	test CanPatchSKUERCViaAPI {
		property portal.acceptance = "false";

		var skuId = CommerceJSONProductsAPI._getSKUId(productSKU = "MIN93015");

		task ("Given a Minium site is created") {
			CommerceAccelerators.initializeNewSiteViaAccelerator(siteName = "Minium");
