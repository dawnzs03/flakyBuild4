	macro editOrderShipmentDetails {
		if (isSet(carrierName)) {
			Click(
				key_text = "Carrier Details",
				locator1 = "CommerceOrders#EDIT_SHIPMENT_DETAILS");

			CommerceShipments.editShipmentCarrierDetails(
				carrierDetail = ${carrierName},
				shippingMethod = ${carrierShippingMethod},
				trackingNumber = ${carrierTrackingNumber},
				trackingURL = ${carrierBaseTrackingURL});
		}

		if (isSet(shippingAddressName)) {
			Click(
				key_text = "Shipping Address",
				locator1 = "CommerceOrders#EDIT_SHIPMENT_DETAILS");

			CommerceOrders.editOrderAddressCP(
				editedCity = ${shippingAddressCity},
				editedCountry = ${shippingAddressCountry},
				editedName = ${shippingAddressName},
				editedRegion = ${shippingAddressRegion},
				editedStreet1 = ${shippingAddressStreet1},
				editedZipCode = ${shippingAddressZipCode});
		}

		if (isSet(estimatedDeliveryDate)) {
			AssertClickNoError(
				key_buttonName = "Edit",
				key_titleName = "Estimated Delivery Date",
				locator1 = "CommerceOrders#ORDER_GENERAL_DETAILS_ADD_EDIT_BUTTON",
				value1 = "Edit");

			SelectFrame.selectFrameNoLoading(locator1 = "IFrame#MODAL_IFRAME");

			Type(
				locator1 = "CommerceEntry#INVENTORY_EXPECTED_INCOMING_DELIVERY_DATE",
				value1 = ${estimatedDeliveryDate});

			AssertClickNoError(
				locator1 = "Button#SUBMIT",
				value1 = "Submit");

			WaitForPageLoad();
		}

		if (isSet(estimatedShippingDate)) {
			AssertClickNoError(
				key_buttonName = "Edit",
				key_titleName = "Estimated Shipping Date",
