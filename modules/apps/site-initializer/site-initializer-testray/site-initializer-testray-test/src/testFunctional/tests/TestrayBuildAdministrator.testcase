	@description = "Story ID: LPS-158466 | Test Summary: Verify if as an Administrator I can delete Builds"
	@priority = 5
	test AdminCanDeleteBuild {
		task ("Given that I create a build via API and go to build page") {
			JSONTestray.addBuild(
				buildName = ${buildName},
				productVersion = ${productVersion},
				projectName = ${projectName},
				routineName = ${routineName});

			Testray.clickTableEntry(tableEntry = ${projectName});

			Testray.clickTableEntry(tableEntry = ${routineName});

			Testray.viewTableEntryList(listEntry = ${buildName});
		}

		task ("When a build is deleted") {
			Testray.deleteItem(itemName = ${buildName});
		}

		task ("Then the build should not appear on the list") {
			Testray.viewTableEntryListNotPresent(listEntry = ${buildName});
		}
	}

	@description = "Story ID: LPS-158466 | Test Summary: Verify if as an Administrator I should be able to delete a build that contains runs"
	@priority = 4
	test AdminCanDeleteBuildContainsRuns {
		task ("Given that the user add a new build with Environments Factors") {
			Testray.clickTableEntry(tableEntry = ${projectName});

			Testray.clickTableEntry(tableEntry = ${routineName});

			TestrayBuild.addBuildViaUI(
				buildFieldName = ${buildName},
				caseList = ${caseName},
				dropdownItem = "New Build",
				productVersionOption = ${productVersion},
				routine = ${routineName});
		}

		task ("When delete build with runs") {
			Testray.clickTableEntry(tableEntry = ${buildName});

			Navigator.gotoNavTab(navTab = "Runs");

			Testray.clickTableEntry(tableEntry = "Option 1");

			JSONTestrayAPI.deleteObjectViaAPI(
				object = "builds",
				objectName = "Build 1");
		}

		task ("And go to build page") {
			TestrayDropdown.goToResultSidebar();

			Testray.changePagination(quantity = 150);

			Testray.clickTableEntry(tableEntry = ${projectName});

			Testray.clickTableEntry(tableEntry = ${routineName});
		}

		task ("Then the build with runs should not appear on the list") {
			Testray.viewTableEntryListNotPresent(listEntry = ${buildName});
		}
	}

	@description = "Story ID: LPS-158466 | Test Summary: Verify if as an Administrator I am able to edit Builds"
	@priority = 5
	test AdminCanEditBuild {
		task ("Given that the user add a new build") {
			Testray.clickTableEntry(tableEntry = ${projectName});

			Testray.clickTableEntry(tableEntry = ${routineName});

			TestrayBuild.addBuildViaUI(
				buildFieldName = ${buildName},
				caseList = ${caseName},
				dropdownItem = "New Build",
				productVersionOption = ${productVersion},
				routine = ${routineName});
		}

		task ("When a build is edited") {
			TestrayManage.selectTableRowOption(
				rowOption = "Edit",
				tableEntry = ${buildName});

			TestrayBuild.editBuild(
				caseList = ${caseName},
				description = "Description Edited",
				gitHash = 12345,
				newName = "Build 1 Edited",
				productVersion = ${productVersion},
				routine = ${routineName});
		}

		task ("Then the build should appear on the list with edited name") {
			Testray.viewTableEntryList(listEntry = "Build 1 Edited");
		}
	}

	@description = "Story ID: LPS-158466 | Test Summary: Verify if as an Administrator I should not be able to add a duplicate build"
	@priority = 3
	test AdminCanNotAddDuplicateBuild {
		task ("Given that the user add a new build") {
			JSONTestray.addBuild(
				buildName = ${buildName},
				productVersion = ${productVersion},
				projectName = ${projectName},
				routineName = ${routineName});

			Testray.clickTableEntry(tableEntry = ${projectName});

			Testray.clickTableEntry(tableEntry = ${routineName});
		}

		task ("When add a build with duplicated name") {
			TestrayBuild.addBuildViaUI(
				buildFieldName = ${buildName},
				caseList = ${caseName},
				dropdownItem = "New Build",
				productVersionOption = "1.3",
				routine = ${routineName});
		}

		task ("Then a warning message is displayed") {
			Alert.viewErrorMessage(errorMessage = "The Build name already exists");
		}
	}

