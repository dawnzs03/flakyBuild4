package com.liferay.source.formatter.dependencies;

/**
 * @author Hugo Huijser
 */
public class SingleStatementClause {

	public void method(String s) {
		if (s == null)
			s = "Hello World";

		int i = 0;

		while (i < 10)
			i++;

		for (i = 0; i < 10;)
			i++;
			
		if (s.length() > 5)
			return;
	}

}