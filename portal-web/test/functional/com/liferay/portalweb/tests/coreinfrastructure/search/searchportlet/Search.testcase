		task ("Assert the search results are ordered correctly when sorting by the 'date' custom object field") {
			SearchPortlets.chooseSortOption(sortOption = "Object Date");

			SearchResultPortlet.viewSearchResultsSpecificOrder(resultsList = "Banana Fruit,Apple Fruit,Coconut Fruit");
