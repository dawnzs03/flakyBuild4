		task ("Assert the search results are ordered correctly when sorting by the 'title' custom object field in descending order") {
			SearchPortlets.chooseSortOption(sortOption = "Object Title Desc");

			SearchResultPortlet.viewSearchResultsSpecificOrder(resultsList = "Coconut Fruit,Banana Fruit,Apple Fruit");
