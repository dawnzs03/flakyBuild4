@default = "select"
definition {

	function pauseSelect {
		WaitForSPARefresh();

		selenium.pause(5000);

		selenium.select();

		selenium.assertJavaScriptErrors();

		selenium.assertLiferayErrors();
	}

	function select {
		WaitForSPARefresh();

		selenium.waitForEditable();

		selenium.mouseOver();

		selenium.select();

		selenium.assertJavaScriptErrors();

		selenium.assertLiferayErrors();
	}

}